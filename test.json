{
  "swagger": "2.0",
  "info": {
    "description": "Documentation de l'api chympy",
    "version": "1.0.0",
    "title": "API chympy",
    "contact": {
      "email": "theodaco3@gmail.com"
    }
  },
  "host": "chympy.net",
  "basePath": "/api",
  "schemes": [
    "https"
  ],
  "paths": {
    "/particuliers/current": {
      "get": {
        "summary": "Récupère les informations utilisateurs lié au JWT passé dans le header",
        "description": "Récupère les informations utilisateurs lié au JWT passé dans le header\n",
        "operationId": "searchInventory",
        "produces": [
          "application/json"
        ],
        "parameters": [],
        "responses": {
          "200": {
            "description": "search results matching criteria",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/User"
              }
            }
          },
          "400": {
            "description": "bad input parameter"
          }
        }
      }
    },
    "/particuliers/login": {
      "post": {
        "summary": "Connexion utilisateur",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "inventoryItem",
            "description": "Connexion utilisateur",
            "required": false,
            "schema": {
              "$ref": "#/definitions/User"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Utilisateur connecté"
          }
        }
      }
    },
    "/offres/find": {
      "get": {
        "summary": "Récupères les offres chympy",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "offre",
            "description": "Récupération des offres",
            "required": false,
            "schema": {
              "$ref": "#/definitions/Offre"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Offre récupére"
          }
        }
      }
    },
    "/categorie/find": {
      "get": {
        "summary": "Récupères les catégories chympy",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "catégorie",
            "description": "Récupération des catégories",
            "required": false,
            "schema": {
              "$ref": "#/definitions/Categorie"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Catégorie récupére"
          }
        }
      }
    },
    "/transactions/addExtensionTransaction": {
      "post": {
        "summary": "Ajoute une transaction depuis l'extension de navigateur",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "transactions",
            "description": "Ajoute une transaction depuis l'extension de navigateur",
            "required": false,
            "schema": {
              "$ref": "#/definitions/Transaction"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Transaction ajouté"
          }
        }
      }
    }
  },
  "definitions": {
    "User": {
      "type": "object",
      "required": [
        "access_token",
        "address",
        "birthday",
        "city",
        "created_at",
        "email",
        "firstname",
        "gender",
        "id",
        "last_login",
        "last_name",
        "password",
        "phone",
        "postal_code",
        "token"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uuid",
          "example": "d290f1ee-6c54-4b01-90e6-d701748f0851"
        },
        "firstname": {
          "type": "string",
          "example": "Théo"
        },
        "last_name": {
          "type": "string",
          "example": "Da Costa"
        },
        "gender": {
          "type": "string",
          "example": "Da Costa"
        },
        "birthday": {
          "type": "string",
          "format": "date-time",
          "example": "2016-08-29T09:12:33.001Z"
        },
        "last_login": {
          "type": "string",
          "format": "date-time",
          "example": "2016-08-29T09:12:33.001Z"
        },
        "address": {
          "type": "string",
          "example": "25 rue bidon"
        },
        "postal_code": {
          "type": "string",
          "example": "76000"
        },
        "city": {
          "type": "string",
          "example": "Rouen"
        },
        "email": {
          "type": "string",
          "example": "theo@mail.com"
        },
        "password": {
          "type": "string",
          "example": "Je suis un mot de pense chiffré"
        },
        "token": {
          "type": "string",
          "example": "Je suis un token temporaire"
        },
        "access_token": {
          "type": "string",
          "example": "Je suis un token bancaire"
        },
        "phone": {
          "type": "string",
          "example": "0123456789"
        },
        "created_at": {
          "type": "string",
          "format": "date-time",
          "example": "2016-08-29T09:12:33.001Z"
        }
      }
    },
    "Offre": {
      "required": [
        "cashback",
        "conditions",
        "created_at",
        "id",
        "id_company",
        "likes",
        "min_cashback",
        "status",
        "unit",
        "vat"
      ],
      "properties": {
        "id": {
          "type": "string",
          "example": "d290f1ee-6c54-4b01-90e6-d701748f0851"
        },
        "id_company": {
          "type": "string",
          "example": "Objet Entreprise"
        },
        "cashback": {
          "type": "integer",
          "example": 5
        },
        "vat": {
          "type": "integer",
          "example": 5
        },
        "unit": {
          "type": "string",
          "example": "%"
        },
        "conditions": {
          "type": "string",
          "example": "min_value"
        },
        "likes": {
          "type": "integer",
          "example": 5
        },
        "min_cashback": {
          "type": "integer",
          "example": 5
        },
        "created_at": {
          "type": "string",
          "format": "date-time",
          "example": "2016-08-29T09:12:33.001Z"
        },
        "status": {
          "type": "boolean",
          "example": true
        }
      }
    },
    "Entreprise": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer",
          "format": "int64"
        },
        "legal_name": {
          "type": "string",
          "example": "chympy"
        },
        "commercial_name": {
          "type": "string",
          "example": "chympy"
        },
        "financial_name": {
          "type": "string",
          "example": "chympy"
        },
        "address": {
          "type": "string",
          "example": "25 rue bidon"
        },
        "postal_code": {
          "type": "string",
          "example": "76000"
        },
        "city": {
          "type": "string",
          "example": "Rouen"
        },
        "email": {
          "type": "string",
          "example": "chympy@mail.fr"
        },
        "created_at": {
          "type": "string",
          "format": "date-time",
          "example": "2016-08-29T09:12:33.001Z"
        },
        "website": {
          "type": "string",
          "example": "chympy.net"
        },
        "id_category": {
          "type": "string",
          "example": "Objet Catégorie"
        }
      }
    },
    "Categorie": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer",
          "format": "int64"
        },
        "label": {
          "type": "string",
          "example": "Réstauration"
        }
      }
    },
    "Transaction": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer",
          "format": "int64"
        },
        "amount": {
          "type": "integer",
          "example": 55
        },
        "wording": {
          "type": "string",
          "example": "Transaction McDonalds"
        },
        "transaction_date": {
          "type": "string",
          "format": "date-time",
          "example": "2016-08-29T09:12:33.001Z"
        },
        "id_transaction": {
          "type": "string",
          "example": "55989"
        },
        "id_account": {
          "type": "integer",
          "example": 55
        },
        "id_company": {
          "type": "string",
          "example": "Objet Entreprise"
        },
        "id_user": {
          "type": "string",
          "example": "Objet User"
        },
        "created_at": {
          "type": "string",
          "format": "date-time",
          "example": "2016-08-29T09:12:33.001Z"
        },
        "paid": {
          "type": "boolean",
          "example": true
        }
      }
    }
  }
}